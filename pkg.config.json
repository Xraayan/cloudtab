{
  "name": "cloudtab",
  "version": "1.0.0",
  "bin": "backend/src/server.js",
  "pkg": {
    "scripts": [
      "backend/src/**/*.js"
    ],
    "assets": [
      "backend/.env",
      "backend/uploads/**/*",
      "backend/sessions/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  }
}
